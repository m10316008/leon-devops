include crm2_headers_cache.conf;

location /mobile/ {
    return 301 /fnagent/;
}

location = /main.html {
    return 301 /fnagent/;
}

location = /provider.html {
    return 301 /fnagent/;
}

location = / {
    return 301 /fnagent/;
}

location /fnagent/ {
    proxy_cache_valid      200  14d;
    proxy_cache            x_cache_fnagent;

    include crm2_cache_common.conf;
}

location / {
    include crm2_path_proxy_pass.conf;
}
