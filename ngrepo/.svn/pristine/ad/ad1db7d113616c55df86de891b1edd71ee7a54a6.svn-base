                    1717sk.tv	www.1717sk.tv;

        ssl_certificate     certs/1717sk.tv.crt;
        ssl_certificate_key certs/1717sk.tv.key;

        include crm2_frontend_ssl.conf;
    }

        server {    ###### frontend ######
        listen 443 ssl http2;
