include crm2_headers_cache.conf;

location /brand/ {
    proxy_cache_valid      200  14d;
    proxy_cache            x_cache_fn;

    include crm2_frontend_cache_common.conf;
}

location ~* ^/(brand|static|my-account|game-windows)/ {
    proxy_cache_valid      200  14d;
    proxy_cache            x_cache_fn;

    include crm2_frontend_cache_common.conf;
}

